if not game:IsLoaded() then
    game.Loaded:Wait()
end

local creator = game:GetService("MarketplaceService"):GetProductInfo(game.PlaceId).Creator.CreatorTargetId

local games = {
    [7381705] = {
        free = 'loadstring(game:HttpGet("https://raw.githubusercontent.com/thantzy/freesc/refs/heads/main/ThanHubSC"))()',
        premium = 'loadstring(game:HttpGet("https://raw.githubusercontent.com/thantzy/premSC/refs/heads/main/SCprem))()',
    }
}

return function(script_key)
    if games[creator] then
        if script_key and script_key = "" then
            loadstring(game:HttpGet(games[creator].premium))()
        else
            loadstring(game:HttpGet(games[creator].free))()
        end
    end
end
